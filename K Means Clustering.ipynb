    "# IMPORT DATASET THEN SPLIT INDEPENDENT AND DEPENDENT VARIABLE\n",
    "df = pd.read_csv('wine-clustering.csv')\n",
    "df.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "2f234564",
   "metadata": {},
   "outputs": [
